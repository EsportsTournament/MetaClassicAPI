{
    "version": 2,
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "access-control-allow-origin",
                    "value": "*"
                }
            ]
        }
    ],
    "rewrites": [
        { "source": "/(.*)", "destination": "/" }
      ],
    "builds": [
        {
            "src": "./index.js",
            "use": "@now/node"
        }
    ]
}